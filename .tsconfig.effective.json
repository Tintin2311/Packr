{
    "compilerOptions": {
        "allowJs": false,
        "esModuleInterop": true,
        "jsx": "react-jsx",
        "lib": [
            "es2021",
            "dom"
        ],
        "module": "preserve",
        "moduleDetection": "force",
        "moduleResolution": "bundler",
        "customConditions": [
            "react-native"
        ],
        "noEmit": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "target": "es2021",
        "strict": true,
        "baseUrl": "./",
        "paths": {
            "@/*": [
                "./*"
            ]
        },
        "allowSyntheticDefaultImports": true,
        "resolvePackageJsonExports": true,
        "resolvePackageJsonImports": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "strictBindCallApply": true,
        "strictPropertyInitialization": true,
        "strictBuiltinIteratorReturn": true,
        "alwaysStrict": true,
        "useUnknownInCatchVariables": true
    },
    "files": [
        "./card/builder/index.ts",
        "./card/cards/index.ts",
        "./card/engines/index.ts",
        "./card/engines/image/stableDiffusion.ts",
        "./card/engines/prompts/stylePrompt.ts",
        "./card/services/index.ts",
        "./card/theme/elements.ts",
        "./card/theme/grammar.ts",
        "./card/theme/index.ts",
        "./card/theme/rarity.ts",
        "./mon-app/constants/Colors.ts",
        "./mon-app/hooks/useColorScheme.ts",
        "./mon-app/hooks/useColorScheme.web.ts",
        "./mon-app/hooks/useThemeColor.ts",
        "./utils/phone.ts",
        "./App.tsx",
        "./AuthReturnOverlay.tsx",
        "./AuthScreen.tsx",
        "./AuthScreenConnexion.tsx",
        "./AuthScreenInscription.tsx",
        "./AuthScreenRecuperation.tsx",
        "./OuvertureBooster.tsx",
        "./ProfileFormScreen.tsx",
        "./ResetPasswordScreen.tsx",
        "./SupabaseClient.tsx",
        "./Images/PhotoPackConnexion.tsx",
        "./Images/PhotoPackInscription.tsx",
        "./Images/PhotoPackRecuperation.tsx",
        "./card/builder/CardBuilder.tsx",
        "./card/cards/CardTextPerso.tsx",
        "./card/cards/ProfileCard.tsx",
        "./card/theme/RarityBorder.tsx",
        "./mon-app/app/+not-found.tsx",
        "./mon-app/app/_layout.tsx",
        "./mon-app/app/(tabs)/_layout.tsx",
        "./mon-app/app/(tabs)/explore.tsx",
        "./mon-app/app/(tabs)/index.tsx",
        "./mon-app/components/Collapsible.tsx",
        "./mon-app/components/ExternalLink.tsx",
        "./mon-app/components/HapticTab.tsx",
        "./mon-app/components/HelloWave.tsx",
        "./mon-app/components/ParallaxScrollView.tsx",
        "./mon-app/components/ThemedText.tsx",
        "./mon-app/components/ThemedView.tsx",
        "./mon-app/components/ui/IconSymbol.ios.tsx",
        "./mon-app/components/ui/IconSymbol.tsx",
        "./mon-app/components/ui/TabBarBackground.ios.tsx",
        "./mon-app/components/ui/TabBarBackground.tsx",
        "./navigation/AppTabs.tsx",
        "./overlay/LikesOverlay.tsx",
        "./overlay/PhotoCropModal.tsx",
        "./overlay/SettingsOverlay.tsx",
        "./screens/AccueilConnected.tsx",
        "./screens/CardCreationScreen.tsx",
        "./screens/CollectionScreen.tsx",
        "./screens/MessagesScreen.tsx",
        "./screens/ProfileScreen.tsx",
        "./screens/SettingsScreen.tsx",
        "./screens/ShopScreen.tsx"
    ],
    "include": [
        "**/*.ts",
        "**/*.tsx"
    ],
    "exclude": [
        "node_modules",
        "dist",
        "babel.config.js"
    ]
}
